<div class="content">
    <h1>Product List:</h1>
    <div id="listaProductos" class="product-list">
        {{#each products}}
        <div class="product-box">
            <h2>{{this.title}}</h2>
            <p>Description: {{this.description}}</p>
            <p>Price: ${{this.price}}</p>
            <p>Stock: {{this.stock}}</p>
            <p>Code: {{this.code}}</p>
        </div>
        {{/each}}
    </div>
</div>